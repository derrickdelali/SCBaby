<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.scbaby">

    <uses-permission android:name="android.permission.RECORD_AUDIO"></uses-permission>
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SCBaby">
        <activity
            android:name=".CryRecognition"
            android:theme="@style/AppTheme.FullScreen"
            android:exported="true" />
        <activity
            android:name="scbaby.app.scbaby.New_Child"
            android:exported="true"
            android:theme="@style/AppTheme.FullScreen" />
        <activity
            android:name="scbaby.app.scbaby.Feedback_Message"
            android:theme="@style/AppTheme.FullScreen" />
        <activity
            android:name="scbaby.app.scbaby.More_Information"
            android:theme="@style/AppTheme.FullScreen" />
        <activity
            android:name="scbaby.app.scbaby.Record"
            android:theme="@style/AppTheme.FullScreen" />
        <activity
            android:name="scbaby.app.scbaby.Disclaimer"
            android:theme="@style/AppTheme.FullScreen" />
        <activity
            android:name="scbaby.app.scbaby.Login"
            android:theme="@style/AppTheme.FullScreen" />
        <activity
            android:name="scbaby.app.scbaby.Register"
            android:theme="@style/AppTheme.FullScreen" />
        <activity
            android:name="scbaby.app.scbaby.MainActivity"
            android:theme="@style/AppTheme.FullScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>